<h1 class="text-center"> Auction Product Report </h1> 
<table class="table">
  <thead class="thead-dark">
    <tr>
      <th scope="col">Bided Amount </th>
      <th scope="col">Bid AT</th>
    </tr>
  </thead>
  <% @auction_product_report.each do |report| %>
    <tbody>
      <tr>
      <th scope="row"><%=report.bidamount%></th>
      <td><%=report.created_at%></td>
    </tr>
  <% end %>
  </tbody>
</table>


